<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Prevent_same_Depart_Time_Arrival_time</fullName>
    <active>true</active>
    <errorConditionFormula>Departure_Date_Time__c =  Arrival_Date_Time__c</errorConditionFormula>
    <errorMessage>Departure and Arrival time should not be same</errorMessage>
</ValidationRule>
