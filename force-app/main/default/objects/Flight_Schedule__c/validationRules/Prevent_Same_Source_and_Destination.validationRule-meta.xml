<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Prevent_Same_Source_and_Destination</fullName>
    <active>true</active>
    <errorConditionFormula>Text(Destination_Name__c) = Text(Source_Name__c)</errorConditionFormula>
    <errorMessage>Source and destination must be different, Please change</errorMessage>
</ValidationRule>
